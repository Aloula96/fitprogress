{% extends 'baseAdmin.html.twig' %} {% block title %}Edit Workout Plan{%
endblock %} {% block body %}
<div class="container">
    <h1>Edit Workout Plan</h1>

    {{ form_start(form) }}

    <div class="form-group">
        {{ form_row(form.level) }}
    </div>
    <div class="form-group">
        {{ form_row(form.description) }}
    </div>
    <div class="form-group">
        {{ form_row(form.type) }}
    </div>

    <button type="submit" class="btn btn-primary">Save Changes</button>
    <a href="{{ path('admin_workoutplan') }}" class="btn btn-secondary"
        >Back to list</a
    >
    {{ form_end(form) }}
</div>
{% endblock %}
